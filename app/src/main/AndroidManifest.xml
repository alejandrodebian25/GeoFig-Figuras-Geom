<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alejandro.figurasgeometricas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Cuadrado" />
        <activity android:name=".Rectangulo" />
        <activity android:name=".Circulo" />
        <activity android:name=".Pentagono" />
        <activity android:name=".Rombo" />
        <activity android:name=".Triangulo" />
        <activity android:name=".Trapecio" />
        <activity android:name=".TrapecioRecto" />
        <activity android:name=".Paralelogramo"></activity>
    </application>

</manifest>